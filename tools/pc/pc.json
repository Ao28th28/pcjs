{
  "defaults": {
    "type": "pcx86",
    "system": "msdos",
    "version": "3.30",
    "drivesize": "10mb",
    "maxfiles": 1024,
    "directory": ".",
    "machine": "compaq386.json",
    "state": "state386.json"
  },
  "apps": {
    "cp": {},
    "ls": {},
    "vi": {},
    "edit": {
      "exec": "vi $*"
    }
  },
  "systems": {
    "compaq": {
      "vendor": "compaq",
      "product": "compaq-dos",
      "files": ["IBMBIO.COM", "IBMDOS.COM", "COMMAND.COM"]
    },
    "msdos": {
      "vendor": "microsoft",
      "files": ["IO.SYS", "MSDOS.SYS", "COMMAND.COM"]
    },
    "pcdos": {
      "vendor": "ibm",
      "files": ["IBMBIO.COM", "IBMDOS.COM", "COMMAND.COM"],
      "disks": {
        "1.00": "PCDOS100",
        "1.10": "PCDOS110",
        "comments": "this section allows version-specific boot disks to be specified in place of the default disk names that PCjs normally uses"
      }
    }
  }
}